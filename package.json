{
	"name": "beepro-frontend",
	"version": "0.1.154",
	"private": true,
	"scripts": {
		"android": "react-native run-android --variant devDebug",
		"ios": "react-native run-ios --scheme 'BeePro (Development)' --configuration 'Debug (Development)' --simulator 'iPhone 13 Pro'",
		"ios:tablet": "react-native run-ios --scheme 'BeePro (Development)' --configuration 'Debug (Development)' --simulator 'iPad Pro (11-inch) (3rd generation)'",
		"ios:staging": "react-native run-ios --scheme 'BeePro (Staging)' --configuration 'Debug (Staging)'",
		"ios:staging:tablet": "react-native run-ios --scheme 'BeePro (Staging)' --configuration 'Debug (Staging)' --simulator 'iPad Pro (11-inch) (3rd generation)'",
		"ios:prod": "react-native run-ios --scheme 'BeePro (Production)' --configuration 'Debug (Production)'",
		"ios:prod:tablet": "react-native run-ios --scheme 'BeePro (Production)' --configuration 'Debug (Production)' --simulator 'iPad Pro (11-inch) (3rd generation)'",
		"build:ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios'",
		"start": "react-native start"
	},
	"dependencies": {
		"@react-native-async-storage/async-storage": "^1.17.9",
		"react": "17.0.2",
		"react-native": "0.68.2",
		"react-native-device-info": "^10.0.2",
		"react-native-dotenv": "^3.3.1",
		"react-native-gesture-handler": "^2.5.0",
		"react-native-reanimated": "^2.9.1",
		"react-native-unimodules": "^0.14.5"
	},
	"devDependencies": {
		"@babel/core": "^7.12.9",
		"@babel/plugin-proposal-decorators": "^7.17.9",
		"@babel/plugin-transform-named-capturing-groups-regex": "^7.16.8",
		"@babel/runtime": "^7.12.5",
		"@react-native-community/eslint-config": "^2.0.0",
		"@testing-library/jest-native": "^4.0.4",
		"@testing-library/react-native": "^8.0.0",
		"@types/jest": "^26.0.20",
		"@types/react-native": "^0.64.0",
		"@types/react-native-snap-carousel": "^3.8.5",
		"@types/react-native-vector-icons": "^6.4.6",
		"@types/react-test-renderer": "^16.9.2",
		"@types/tinycolor2": "^1.4.3",
		"@typescript-eslint/eslint-plugin": "^3.10.1",
		"@typescript-eslint/parser": "^3.10.1",
